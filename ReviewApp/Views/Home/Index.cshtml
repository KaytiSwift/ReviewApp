@model IEnumerable<ReviewApp.Models.Review>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
    <link rel="stylesheet" href="~/StyleSheet.css"/>
</head>
<body>
    <h1>Reviews</h1>
    
    @foreach (var review in Model)
    {
        <div>
            <a href="/home/details/@review.ID">@review.Title</a>
        </div>
    }
</body>
</html>
